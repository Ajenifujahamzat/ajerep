//Program to calculate three numbers
   //Declare the class
   public class Calculator{
     public static void main(String [] args) {
            int num1 = 4;
            int num2 = 8;
            int num3 = 12;
            int sum = 0;
            sum = num1 + num2 + num3;
            System.out.print(sum);
        }
}
